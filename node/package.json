  },
  "dependencies": {
    "@vue/server-renderer": "^3.5.0",
    "express": "^4.19.2",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "vite": "^5.4.0"
  }
}
