{
  "name": "svelte-inject-test",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build && NODE_ENV=production node server.js",
    "build": "npm run build:client && npm run build:ssr",
    "build:client": "BUILD_TARGET=client vite build",
    "build:ssr": "BUILD_TARGET=ssr vite build --ssr",
    "serve": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "svelte": "^4.2.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "vite": "^5.4.0"
  }
}

